
<h4 align="center">应聘人员详情</h4>

    <table border="1" align="center">
      <tr>
        <td class="td_style"><%= :姓名%></td><td><%= @employee_info.name%></td>
        <td class="td_style"><%=  :性别%></td><td><%= @employee_info.gender%></td>
        <td class="td_style"><%=  :出生日期%></td><td><%= @employee_info.birth_date%></td>
        <td class="td_style"><%=  :婚育状况%></td><td><%= @employee_info.marriage%></td>
      </tr>
      <tr>
        <td class="td_style"><%=  :民族%></td><td><%= @employee_info.nation%></td>
        <td class="td_style"><%= :籍贯%></td><td><%= @employee_info.origin%></td>
        <td class="td_style"><%=  :政治面貌%></td><td><%= @employee_info.political_landscape%></td>
        <td class="td_style"><%=  :身体状况%></td><td><%= @employee_info.physical_condition%></td>
      </tr>
      <tr>
        <td class="td_style"><%=  :身高%></td><td><%= @employee_info.height%></td>
        <td class="td_style"><%=  :体重%></td><td><%= @employee_info.weight%></td>
        <td class="td_style"><%=  :联系电话%></td><td><%= @employee_info.contact_phone%></td>
        <td class="td_style" colspan="2">
      </tr>
      <tr>
        <td class="td_style"><%=  :毕业时间%></td><td><%= @employee_info.graduation_time%></td>
        <td class="td_style"><%=  :毕业院校%></td><td><%= @employee_info._school%></td>
        <td class="td_style"><%=  :专业%></td><td><%= @employee_info.major%></td>
        <td class="td_style"><%= :最高学历%></td><td><%=@employee_info.highest_degree%></td>
      </tr>
      <tr>
        <td colspan="2"><center><%=  :身份证号码%></center></td><td colspan="2" class="yuan_style" ><%= @employee_info.id_card_number%></td>
        <td colspan="2"><center><%= :户籍地址%></center><td colspan="2" class="yuan_style"><%= @employee_info.permanent_address%></td>
      </tr>
      <tr>
        <td colspan="2"><center><%=  :现居住地址%></center><td colspan="2" class="yuan_style"><%= @employee_info.live_address%></td>
        <td colspan="2"><center><%= :邮箱%></center><td colspan="2" class="yuan_style"><%= @employee_info.mailbox%></td>
      </tr>
      <tr>
        <td colspan="2"><center><%=  :期望薪资%></center><td colspan="2" class="yuan_style"><%= @employee_info.salary_expectation%></td>
        <td colspan="2"><center><%=  :到岗时间%></center><td colspan="2" class="yuan_style"><%=@employee_info.arrival_time%></td>
      </tr>
      <tr>
        <td colspan="8"><h4 align="center">工作经历</h4></td>
      </tr>
       <% @work_params.each do |ww| %>
          <tr>
            <td class="jingli_style" colspan="2" align="center"><%= :起止年月%></td>
            <td class="jingli_style" ><%= :工作单位%></td>
            <td class="jingli_style" ><%= :岗位%></td>
            <td class="jingli_style"><%= :薪资%></td>
            <td class="jingli_style"><%=  :离职原因%></td>
            <td class="jingli_style" ><%=  :证明人%></td>
            <td class="jingli_style" ><%=  :电话%>
          </tr>
          <tr>
            <td class="jingli_style"><%= ww.start_and%></td><td><%= ww.end_date%></td>
            <td class="jingli_style"><%= ww.work_unit%></td>
            <td class="jingli_style"><%= ww.post%></td>
            <td class="jingli_style"><%= ww.salary%></td>
            <td class="jingli_style"><%= ww.reasons_for_leaving%></td>
            <td class="jingli_style"><%= ww.witness%></td>
            <td class="jingli_style"><%= ww.telephone%>
          </tr>
          <tr id="addtable"></tr>
      <% end %>
      <tr id="stop">
        <td colspan="8"><h4 align="center">教育情况</h4></td>
      </tr>
    <% @cducation.each do |cc| %>
          <tr>
            <td class="jingli_style" colspan="2" align="center"><%=  :起止年月%></td>
            <td class="jingli_style" colspan="1"><%= :就读学校%></td>
            <td class="jingli_style" colspan="2"><%=  :专业%></td>
            <td class="jingli_style"><%=  :学习形式%></td>
            <td class="jingli_style" colspan="2"><%=  :所获学历及学位%></td>
          </tr>

          <tr>
            <td class="jingli_style"><%= cc.start_and%></td><td><%= cc.end_date%></td>
            <td class="jingli_style" colspan="1"><%= cc.school%></td>
            <td class="yuan_style" colspan="2"><%= cc.major%></td>
            <td class="jingli_style"><%= cc.leaming_form%></td>
            <td class="yuan_style" colspan="2"><%= cc.qualifications_and_degree%></td></td>
          </tr>
          <tr id="addtable2"> </tr>
      <% end%>
      <tr>
        <td class="jingli_style" colspan="2" align="center"><%= :熟练使用办公软件%></td>
        <td colspan="6">
          <%= @employee_info.office_software%>

        </td>
      </tr>
      <tr>
        <td class="jingli_style">应聘理由:</td>
        <td colspan="7">
          <%= @employee_info.reasons_for_applying%>
        </td>
      </tr>
      <tr>
        <td><%=  "最能概括自己的3个字:"%></td><td class="yuan_style" colspan="7"><%=@employee_info.self_evaluation%></td>
      </tr>

      <tr>
          <td colspan="2" align="center"><%= link_to '返回' , :controller => 'admin/staffinfo' , :action => 'index'%></td>
        <td  colspan="3" align="center"><%= link_to '修改' , :controller => 'admin/staffinfo' , :action => 'edit'%></td>
        <td colspan="3" align="center"> <%= link_to '删除', {:controller => 'admin/staffinfo', :action => 'destroy', :id => "#{@employee_info.id}"}%></td>
      </tr>
    </table>

